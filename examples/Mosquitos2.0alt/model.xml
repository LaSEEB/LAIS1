<LAISModel numThreads="4" id="model">
	<spaceFactory class="org.laseeb.LAIS.space.SquareSpaceFactory">
		<x>100</x>
		<y>100</y>
		<torus>true</torus>
	</spaceFactory>
	<dt>1</dt>
	<dx2>1</dx2>
	<minConThreshold>0.00000001</minConThreshold>
	<substanceMan>
		<families>
		<family id="pheroFamily">
				<name>Pheromone family</name>
				<subMaxColorImpactCon>1</subMaxColorImpactCon>
			</family>
		</families>
		<substances>
			<substance id="pheromone" kDif="0.3" kDeg="-0.05">
				<name>Pheromone</name>
				<bitIdentifier>0</bitIdentifier> <!-- 0000 0000h -->
				<family reference="pheroFamily"/>
				<color>200,50,50</color>
			</substance>
			<substance id="Oviprox" kDif="0.3" kDeg="-0.05">
				<name>Humidity</name>
				<bitIdentifier>0</bitIdentifier> <!-- 0000 0000h -->
				<family reference="pheroFamily"/>
				<color>100,100,255</color>
			</substance>
			<substance id="AdultPhero" kDif="0.0" kDeg="1">
				<name>adultPhero</name>
				<bitIdentifier>0</bitIdentifier> <!-- 0000 0000h -->
				<family reference="pheroFamily"/>
				<color>0,250,0</color>
			</substance>
			<substance id="toxic" kDif="0.001" kDeg="-0.5">
				<name>Toxicity</name>
				<bitIdentifier>0</bitIdentifier> <!-- 0000 0000h -->
				<family reference="pheroFamily"/>
				<color>0,0,0</color>
			</substance>
			<substance id="Heat" kDif="0.3" kDeg="-0.05">
				<name>BodyHeat</name>
				<bitIdentifier>0</bitIdentifier> <!-- 0000 0000h -->
				<family reference="pheroFamily"/>
				<color>250,150,0</color>
			</substance>
		</substances>
		<subFamilyMergeRules>
		</subFamilyMergeRules>
	</substanceMan>
	<agentMan>
		<!--***********************
		Oviposition Container
		**************************-->
		<agentDefinition name="OviContainer">
			<agentPrototype id="oviContain">
				<genome>
					<gene>
						<conditions>
						</conditions>
						<actions>
							<AgentAction class="org.laseeb.LAIS.agent.actions.ProdConsSubAction">
								<conBase>1</conBase>
								<localOrSuperficial>true</localOrSuperficial> <!-- Produced locally -->
								<substance>OviRef</substance>
							</AgentAction>
						</actions>
					</gene>
					<gene><!-- Oviposition spots are less toxic -->
						<conditions>
							<AgentCondition class="org.laseeb.LAIS.agent.conditions.SubConCondition">
								<index>0</index>
								<minQuantity>0.01</minQuantity>
								<substance>toxic</substance>
							</AgentCondition> 
						</conditions>
						<actions>
							<AgentAction class="org.laseeb.LAIS.agent.actions.ProdConsSubAction">
								<conBase>-0.5</conBase>
								<indexCon>0</indexCon>
								<localOrSuperficial>true</localOrSuperficial> <!-- Oviposition spots are less toxic -->
								<substance>toxic</substance>
							</AgentAction>
						</actions>
					</gene>
				</genome>
				<refSubMap>
					<entry>
						<string>OviRef</string>
						<SubstanceProxy>
							<substance reference="Oviprox"/>
						</SubstanceProxy>
					</entry>
					<entry>
						<string>toxic</string>
						<SubstanceProxy>
							<substance reference="toxic"/>
						</SubstanceProxy>
					</entry>
				</refSubMap>
				<states>
				</states>
				<color>0,0,100</color>
			</agentPrototype>
		</agentDefinition>
		<!--
		*************************
		Human
		*************************
		-->
		<agentDefinition name="Human">
			<agentPrototype id="human">
				<genome>
					<gene> <!--Moving Randomly-->
						<conditions>
							<AgentCondition class="org.laseeb.LAIS.agent.conditions.RandomWalkCondition">
								<index>0</index>
							</AgentCondition>
						</conditions>
						<actions>
							<AgentAction class="org.laseeb.LAIS.agent.actions.WalkAction">
								<index>0</index>
							</AgentAction>
						</actions>
					</gene>
					<gene> <!--Pheromone-->
						<conditions>
						</conditions>
						<actions>
							<AgentAction class="org.laseeb.LAIS.agent.actions.ProdConsSubAction">
								<conBase>1</conBase>
								<localOrSuperficial>true</localOrSuperficial> <!-- Produced locally -->
								<substance>heatRef</substance>
							</AgentAction>
						</actions>
					</gene>
				</genome>
				<refSubMap>
					<entry>
						<string>heatRef</string>
						<SubstanceProxy>
							<substance reference="Heat"/>
						</SubstanceProxy>
					</entry>
				</refSubMap>
				<states>
					<stateType type="Infected">Yes,No</stateType>
				</states>
				<color>150,100,50</color>
			</agentPrototype>
		</agentDefinition>
		<!--
		*************************
		Wild Male
		*************************
		-->
		<agentDefinition name="WildMale">
			<agentPrototype id="wild">
				<genome>
					<gene> <!--Adult Starting adults-->
						<conditions>
							<AgentCondition class="org.laseeb.LAIS.agent.conditions.StateCondition">
								<stateType>State</stateType>
								<stateValues length="1">
									<string>SAdult</string>
								</stateValues>
							</AgentCondition>
						</conditions>
						<actions>
							<AgentAction class="org.laseeb.LAIS.agent.actions.ChangeStateAction">
								<stateType>State</stateType>
								<stateValue>Adult</stateValue>
							</AgentAction>
							<AgentAction class="org.laseeb.LAIS.agent.actions.ProdConsSubAction">
								<conBase>10.1</conBase>
								<localOrSuperficial>false</localOrSuperficial> <!-- place substance on self -->
								<substance>adultRef</substance>
							</AgentAction>
						</actions>
					</gene>
					<gene> <!--Becomes Adult-->
						<conditions>
							<AgentCondition class="org.laseeb.LAIS.agent.conditions.StochasticCondition">
								<probability>0.9</probability> 
							</AgentCondition>
							<AgentCondition class="org.laseeb.LAIS.agent.conditions.StateCondition">
								<stateType>State</stateType>
								<stateValues length="1">
									<string>Pupa</string>
								</stateValues>
							</AgentCondition>
						</conditions>
						<actions>
							<AgentAction class="org.laseeb.LAIS.agent.actions.ChangeStateAction">
								<stateType>State</stateType>
								<stateValue>Adult</stateValue>
							</AgentAction>
							<AgentAction class="org.laseeb.LAIS.agent.actions.ProdConsSubAction">
								<conBase>10.1</conBase>
								<localOrSuperficial>false</localOrSuperficial> <!-- place substance on self -->
								<substance>adultRef</substance>
							</AgentAction>
						</actions>
					</gene>
					<gene> <!--Becomes Pupa-->
						<conditions>
							<AgentCondition class="org.laseeb.LAIS.agent.conditions.StochasticCondition">
								<probability>0.8</probability> 
							</AgentCondition>
							<AgentCondition class="org.laseeb.LAIS.agent.conditions.StateCondition">
								<stateType>State</stateType>
								<stateValues length="1">
									<string>Larva</string>
								</stateValues>
							</AgentCondition>
						</conditions>
						<actions>
							<AgentAction class="org.laseeb.LAIS.agent.actions.ChangeStateAction">
								<stateType>State</stateType>
								<stateValue>Pupa</stateValue>
							</AgentAction>
						</actions>
					</gene>
					<gene> <!--Mortality rate for new mosquitos-->
						<conditions>
							<AgentCondition class="org.laseeb.LAIS.agent.conditions.StateCondition">
								<stateType>State</stateType>
								<stateValues length="1">
									<string>Egg</string>
								</stateValues>
							</AgentCondition>
							<AgentCondition class="org.laseeb.LAIS.agent.conditions.StochasticCondition">
								<probability>0.15</probability> <!--Mortality Rate-->
							</AgentCondition>
						</conditions>
						<actions>
							<AgentAction class="org.laseeb.LAIS.agent.actions.ApoptosisAction">
							</AgentAction>
						</actions>
					</gene>
					<gene> <!--Becomes Larva-->
						<conditions>
							<AgentCondition class="org.laseeb.LAIS.agent.conditions.StateCondition">
								<stateType>State</stateType>
								<stateValues length="1">
									<string>Egg</string>
								</stateValues>
							</AgentCondition>
							<AgentCondition class="org.laseeb.LAIS.agent.conditions.StochasticCondition">
								<probability>0.11</probability> <!--Maturing Chance-->
							</AgentCondition>
						</conditions>
						<actions>
							<AgentAction class="org.laseeb.LAIS.agent.actions.ChangeStateAction">
								<stateType>State</stateType>
								<stateValue>Larva</stateValue>
							</AgentAction>
						</actions>
					</gene>
					<gene> <!--Looks for Females-->
