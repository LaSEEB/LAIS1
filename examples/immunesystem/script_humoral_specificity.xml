<LAISScript>
	<!-- 
	*******************************
	****** INITIAL CONDITIONS *****
	*******************************
	-->
	<scriptedEvent>
		<ScriptingType class="org.laseeb.LAIS.event.ScheduleAtTick">
			<tick>0</tick>
		</ScriptingType>
		<Event class="org.laseeb.LAIS.event.agdeploy.AgentDeploy">
			<agentPrototype>Th cell</agentPrototype>
			<agStateMap>
				<entry stateType="Phase">Naive</entry>
			</agStateMap>
			<mutRateConstrain class="org.laseeb.LAIS.event.agdeploy.FixedMutationRateConstrain">
				<mutationRate>0.5</mutationRate>
			</mutRateConstrain>
			<iscConstrain class="org.laseeb.LAIS.event.agdeploy.FixedInitSupSubConConstrain">
				<substance name="tcr.cd4">
					10.0
				</substance>
			</iscConstrain>
			<quantityConstrain class="org.laseeb.LAIS.event.agdeploy.FixedQuantityConstrain">
				<quantity>90</quantity>
			</quantityConstrain>
		</Event>
	</scriptedEvent>
	<scriptedEvent>
		<ScriptingType class="org.laseeb.LAIS.event.ScheduleAtTick">
			<tick>0</tick>
		</ScriptingType>
		<Event class="org.laseeb.LAIS.event.agdeploy.AgentDeploy">
			<agentPrototype>APC cell</agentPrototype>
			<agStateMap>
				<entry stateType="Phase">Activated</entry>
			</agStateMap>
			<iscConstrain class="org.laseeb.LAIS.event.agdeploy.FixedInitSupSubConConstrain">
				<substance name="FcR">
					10.0
				</substance>
			</iscConstrain>
			<quantityConstrain class="org.laseeb.LAIS.event.agdeploy.FixedQuantityConstrain">
				<quantity>15</quantity>
			</quantityConstrain>
		</Event>
	</scriptedEvent>
	<scriptedEvent>
		<ScriptingType class="org.laseeb.LAIS.event.ScheduleAtTick">
			<tick>0</tick>
		</ScriptingType>
		<Event class="org.laseeb.LAIS.event.agdeploy.AgentDeploy">
			<agentPrototype>B cell</agentPrototype>
			<agStateMap>
				<entry stateType="Phase">Naive</entry>
			</agStateMap>
			<mutRateConstrain class="org.laseeb.LAIS.event.agdeploy.FixedMutationRateConstrain">
				<mutationRate>0.5</mutationRate>
			</mutRateConstrain>
			<iscConstrain class="org.laseeb.LAIS.event.agdeploy.FixedInitSupSubConConstrain">
				<substance name="ab">
					10.0
				</substance>
			</iscConstrain>
			<quantityConstrain class="org.laseeb.LAIS.event.agdeploy.FixedQuantityConstrain">
				<quantity>45</quantity>
			</quantityConstrain>
		</Event>
	</scriptedEvent>
	<!--
	***********************************
	***** Equilibrium conditions ******
	***********************************
	-->
	<scriptedEvent>
		<ScriptingType class="org.laseeb.LAIS.event.ScheduleAtInterval">
			<interval>1</interval>
		</ScriptingType>
		<Event class="org.laseeb.LAIS.event.agdeploy.AgentDeploy">
			<agentPrototype>Th cell</agentPrototype>
			<agStateMap>
				<entry stateType="Phase">Naive</entry>
			</agStateMap>
			<mutRateConstrain class="org.laseeb.LAIS.event.agdeploy.FixedMutationRateConstrain">
				<mutationRate>0.5</mutationRate>
			</mutRateConstrain>
			<iscConstrain class="org.laseeb.LAIS.event.agdeploy.FixedInitSupSubConConstrain">
				<substance name="tcr.cd4">
					10.0
				</substance>
			</iscConstrain>
			<quantityConstrain class="org.laseeb.LAIS.event.agdeploy.RandomQuantityConstrain">
				<maxQuantity>1</maxQuantity>
			</quantityConstrain>
		</Event>
	</scriptedEvent>
	<scriptedEvent>
		<ScriptingType class="org.laseeb.LAIS.event.ScheduleAtInterval">
			<interval>6</interval>
		</ScriptingType>
		<Event class="org.laseeb.LAIS.event.agdeploy.AgentDeploy">
			<agentPrototype>APC cell</agentPrototype>
			<agStateMap>
				<entry stateType="Phase">Activated</entry>
			</agStateMap>
			<iscConstrain class="org.laseeb.LAIS.event.agdeploy.FixedInitSupSubConConstrain">
				<substance name="FcR">
					10.0
				</substance>
			</iscConstrain>
			<quantityConstrain class="org.laseeb.LAIS.event.agdeploy.FixedQuantityConstrain">
				<quantity>1</quantity>
			</quantityConstrain>
		</Event>
	</scriptedEvent>
	<scriptedEvent>
		<ScriptingType class="org.laseeb.LAIS.event.ScheduleAtInterval">
			<interval>2</interval>
		</ScriptingType>
		<Event class="org.laseeb.LAIS.event.agdeploy.AgentDeploy">
			<agentPrototype>B cell</agentPrototype>
			<agStateMap>
				<entry stateType="Phase">Naive</entry>
			</agStateMap>
			<mutRateConstrain class="org.laseeb.LAIS.event.agdeploy.FixedMutationRateConstrain">
				<mutationRate>0.5</mutationRate>
			</mutRateConstrain>
			<iscConstrain class="org.laseeb.LAIS.event.agdeploy.FixedInitSupSubConConstrain">
				<substance name="ab">
					10.0
				</substance>
			</iscConstrain>
			<quantityConstrain class="org.laseeb.LAIS.event.agdeploy.RandomQuantityConstrain">
				<maxQuantity>1</maxQuantity>
			</quantityConstrain>
		</Event>
	</scriptedEvent>
	<!-- 
	*******************************
	***** Bug insertion ******
	*******************************
	-->	
	<scriptedEvent>
		<ScriptingType class="org.laseeb.LAIS.event.ScheduleAtTick">
			<tick>100</tick>
		</ScriptingType>
		<Event class="org.laseeb.LAIS.event.agdeploy.AgentDeploy">
			<agentPrototype>Bacteria A</agentPrototype>
			<iscConstrain class="org.laseeb.LAIS.event.agdeploy.FixedInitSupSubConConstrain">
				<substance name="ag">
					10
				</substance>
			</iscConstrain>
			<quantityConstrain class="org.laseeb.LAIS.event.agdeploy.FixedQuantityConstrain">
				<quantity>45</quantity>
			</quantityConstrain>
		</Event>
	</scriptedEvent> 
	<scriptedEvent>
		<ScriptingType class="org.laseeb.LAIS.event.ScheduleAtTick">
			<tick>700</tick>
		</ScriptingType>
		<Event class="org.laseeb.LAIS.event.agdeploy.AgentDeploy">
			<agentPrototype>Bacteria B</agentPrototype>
			<iscConstrain class="org.laseeb.LAIS.event.agdeploy.FixedInitSupSubConConstrain">
				<substance name="HA1">
					10
				</substance>
			</iscConstrain>
			<quantityConstrain class="org.laseeb.LAIS.event.agdeploy.FixedQuantityConstrain">
				<quantity>45</quantity>
			</quantityConstrain>
		</Event>
	</scriptedEvent> 

</LAISScript>